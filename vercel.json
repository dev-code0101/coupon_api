{
  "version": 2,
  "builds": [
    {
      "src": "coupon_api/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "installCommand": "python3.9 -m pip install -r requirements.txt",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "coupon_api/wsgi.py"
    }
  ]
}
